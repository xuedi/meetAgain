{% extends 'admin/base.html.twig' %}

{% block content %}

    <h1 class="title is-4">Result</h1>

    <table class="table">
        <tbody>
        <tr>
            <td>Files cleaned up:</td>
            <td>{{ result.cleanedUp }}</td>
        </tr>
        <tr>
            <td>Translations published:</td>
            <td>{{ result.published }}</td>
        </tr>
        <tr>
            <td>Languages:</td>
            <td>{{ result.languages|join(', ') }}</td>
        </tr>
        </tbody>
    </table>

{% endblock %}

