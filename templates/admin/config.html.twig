{% extends 'admin/base.html.twig' %}

{% block title %}Admin - Config{% endblock %}

{% block content %}

    <div class="container">

        <table class="table">
            <thead>
            <tr>
                <th>Name</th>
                <th>Type</th>
                <th>Value</th>
            </tr>
            </thead>
            <tbody>
            {% for item in config %}
                <tr>
                    <th>{{ item.name }}</th>
                    <th>{{ item.type.value }}</th>
                    <th>{{ item.value }}</th>
                </tr>
            {% endfor %}
            </tbody>
        </table>

    </div>

{% endblock %}

