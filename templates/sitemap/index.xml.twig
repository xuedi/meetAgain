<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
{% for site in sites %}
    <url>
        <loc>{{ site.loc }}</loc>
        <lastmod>{{ site.lastmod }}</lastmod>
        <priority>{{ site.prio }}</priority>
    </url>
{% endfor %}
</urlset>