{
  "type": "project",
  "name": "xuedi/meetagain",
  "description": "Simple PHP app to organize a group and schedule recurring events ",
  "license": "EUPL-1.2",
  "authors": [
    {
      "name": "Daniel Koch",
      "email": "xuedi@beijingcode.org",
      "role": "lead"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "beberlei/doctrineextensions": "^1.5",
    "doctrine/data-fixtures": "^2",
    "doctrine/dbal": "^4",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-fixtures-bundle": "^4",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.3",
    "knplabs/knp-time-bundle": "^2.4",
    "nikic/php-parser": "^5.3",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^2.0",
    "rlanvin/php-rrule": "^2.5",
    "sentry/sentry-symfony": "^5.2",
    "symfony/cache": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/expression-language": "7.3.*",
    "symfony/filesystem": "7.3.*",
    "symfony/flex": "^2",
    "symfony/form": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/intl": "7.3.*",
    "symfony/mailer": "7.3.*",
    "symfony/mailgun-mailer": "7.3.*",
    "symfony/mime": "7.3.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/notifier": "7.3.*",
    "symfony/process": "7.3.*",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/rate-limiter": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/stopwatch": "7.3.*",
    "symfony/string": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/validator": "7.3.*",
    "symfony/web-link": "7.3.*",
    "symfony/yaml": "7.3.*",
    "symfonycasts/verify-email-bundle": "^1.17",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Plugin\\": "plugins/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "replace": {
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpunit": "vendor/bin/phpunit -c tests/phpunit.xml"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  },
  "require-dev": {
    "ext-xdebug": "*",
    "phpstan/phpstan": "^2",
    "phpunit/phpunit": "^12",
    "rector/rector": "^2",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/browser-kit": "7.3.*",
    "symfony/debug-bundle": "7.3.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "7.3.*",
    "symfony/web-profiler-bundle": "7.3.*",
    "vimeo/psalm": "^6.10"
  }
}
