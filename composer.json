{
  "type": "project",
  "name": "xuedi/meetagain",
  "description": "Simple PHP app to organize a group and schedule recurring events ",
  "license": "EUPL-1.2",
  "authors": [
    {
      "name": "Daniel Koch",
      "email": "xuedi@beijingcode.org",
      "role": "lead"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-redis": "*",
    "beberlei/doctrineextensions": "^1.5",
    "doctrine/data-fixtures": "^2",
    "doctrine/dbal": "^4",
    "doctrine/doctrine-bundle": "^3.0",
    "doctrine/doctrine-fixtures-bundle": "^4",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.3",
    "knplabs/knp-time-bundle": "^2.4",
    "nikic/php-parser": "^5",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^2.0",
    "rlanvin/php-rrule": "^2.6",
    "sensiolabs/ansi-to-html": "^2.0",
    "sentry/sentry-symfony": "^5.4",
    "symfony/cache": "8.0.*",
    "symfony/console": "8.0.*",
    "symfony/css-selector": "8.0.*",
    "symfony/dotenv": "8.0.*",
    "symfony/expression-language": "8.0.*",
    "symfony/filesystem": "8.0.*",
    "symfony/flex": "^2",
    "symfony/form": "8.0.*",
    "symfony/framework-bundle": "8.0.*",
    "symfony/http-client": "8.0.*",
    "symfony/intl": "8.0.*",
    "symfony/mailer": "8.0.*",
    "symfony/mime": "8.0.*",
    "symfony/monolog-bundle": "^4",
    "symfony/process": "8.0.*",
    "symfony/property-access": "8.0.*",
    "symfony/property-info": "8.0.*",
    "symfony/rate-limiter": "8.0.*",
    "symfony/runtime": "8.0.*",
    "symfony/security-bundle": "8.0.*",
    "symfony/sendgrid-mailer": "8.0.*",
    "symfony/serializer": "8.0.*",
    "symfony/stopwatch": "8.0.*",
    "symfony/string": "8.0.*",
    "symfony/translation": "8.0.*",
    "symfony/twig-bundle": "8.0.*",
    "symfony/validator": "8.0.*",
    "symfony/web-link": "8.0.*",
    "symfony/yaml": "8.0.*",
    "symfonycasts/verify-email-bundle": "^1.17",
    "twig/extra-bundle": "^3",
    "twig/twig": "^3"
  },
  "config": {
    "allow-plugins": {
      "carthage-software/mago": true,
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Plugin\\Glossary\\": "plugins/glossary/src/",
      "Plugin\\Karaoke\\": "plugins/karaoke/src/",
      "Plugin\\Dishes\\": "plugins/dishes/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "replace": {
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpunit": "vendor/bin/phpunit -c tests/phpunit.xml"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "8.0.*"
    }
  },
  "require-dev": {
    "ext-xdebug": "*",
    "carthage-software/mago": "^0.26.1",
    "phpstan/phpstan": "^2",
    "phpunit/phpunit": "^12",
    "rector/rector": "^2",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "4.*",
    "symfony/browser-kit": "8.0.*",
    "symfony/debug-bundle": "8.0.*",
    "symfony/maker-bundle": "^1",
    "symfony/phpunit-bridge": "8.0.*",
    "symfony/web-profiler-bundle": "8.0.*"
  }
}
